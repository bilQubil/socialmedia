<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Landing Page</title>
  </head>
  <body>
    <h1>Posts</h1>
    <ul>
      <% posts.forEach(post => { %>
      <li>
        <h2><%= post.title %></h2>
        <p><%= post.content %></p>
        <img src="<%= post.imgUrl %>" alt="<%= post.title %>" />
        <p>By: <%= post.User.username %> | Tag: <%= post.Tag.name %></p>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
